    <dc:title>European Handbook of Crowdsourced Geographic Information</dc:title>
    <dc:identifier>http://library.oapen.org/bitstream/20.500.12657/32079/1/617901.pdf</dc:identifier>
    <dc:identifier>https://www.doabooks.org/doab?func=search&amp;query=rid:19816</dc:identifier>    <dc:identifier>ISBN: 9781909188792</dc:identifier>
