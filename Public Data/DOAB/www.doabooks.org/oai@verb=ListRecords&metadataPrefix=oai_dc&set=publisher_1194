    <dc:title>Ebooks in education</dc:title>
    <dc:identifier>http://library.oapen.org/bitstream/20.500.12657/33214/1/533880.pdf</dc:identifier>
    <dc:identifier>https://www.doabooks.org/doab?func=search&amp;query=rid:16636</dc:identifier>    <dc:identifier>ISBN: 9781909188372</dc:identifier>
