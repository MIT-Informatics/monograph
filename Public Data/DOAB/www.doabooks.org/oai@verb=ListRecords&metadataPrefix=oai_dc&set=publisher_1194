    <dc:title>Reading With My Eyes Open</dc:title>
    <dc:identifier>http://library.oapen.org/bitstream/20.500.12657/33210/1/533884.pdf</dc:identifier>
    <dc:identifier>https://www.doabooks.org/doab?func=search&amp;query=rid:15580</dc:identifier>    <dc:identifier>ISBN: 9781909188211</dc:identifier>
